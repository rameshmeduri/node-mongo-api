{
  "name": "api-access-manager",
  "version": "1.0.0",
  "author": "AGS",
  "private": true,
  "description": "Kong configuration UI",
  "proxy": "http://localhost:8081",
  "engines": {
    "node": "^8.9.0",
    "npm": "^5.5.0"
  },
  "scripts": {
    "prebuild": "rimraf ./dist && mkdir -p dist/public",
    "build": "NODE_ENV=production npm run build:server && npm run build:client",
    "build:server": "babel --ignore **/*.spec.js src/server/ --out-dir dist/ --source-maps --copy-files",
    "build:client": "node src/client/build/scripts/build.js",
    "start": "node dist/index.js",
    "dev": "npm-run-all --parallel client server",
    "dev:watch": "npm-run-all --parallel client server:watch lint:watch",
    "server": "babel-node src/server/index.js",
    "server:watch": "./node_modules/nodemon/bin/nodemon.js --exec npm run server",
    "client": "node src/client/build/scripts/start.js",
    "dev:docker": "npm-run-all --parallel kong:docker app:docker",
    "app:docker": "docker-compose up",
    "kong:docker": "cd kong-local && docker-compose up",
    "lint": "esw --ignore-path .gitignore src/",
    "lint:watch": "npm run lint -- --watch",
    "pretest": "npm run lint",
    "test": "npm-run-all --parallel test:e2e test:spec test:client",
    "test:watch": "npm-run-all --parallel test:e2e:watch test:spec:watch test:client",
    "test:client": "node src/client/build/scripts/test.js --env=jsdom",
    "spec:coverage": "HTTP_PROXY= BABEL_ENV=test nyc _mocha -- --compilers js:babel-register --colors --reporter spec src/server/**/*.spec.js",
    "test:e2e": "HTTP_PROXY= BABEL_ENV=test _mocha --compilers js:babel-register -R spec test/**/*.e2e.js",
    "test:e2e:watch": "npm run test:e2e -- --watch",
    "test:spec": "HTTP_PROXY= BABEL_ENV=test _mocha --compilers js:babel-register -R spec src/**/*.spec.js",
    "test:spec:watch": "npm run test:spec -- --watch"
  },
  "pre-commit": [
    "lint",
    "test:spec"
  ],
  "dependencies": {
    "@axa-asia/api-common": "^1.0.1",
    "axios": "^0.17.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.1",
    "chalk": "1.1.3",
    "compression": "^1.7.0",
    "connect-mongo": "^2.0.0",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "express-session": "^1.15.6",
    "fs-extra": "3.0.1",
    "helmet": "^3.8.1",
    "history": "^4.7.2",
    "jest": "20.0.4",
    "jsonwebtoken": "^8.1.0",
    "mongoose": "^4.8.4",
    "morgan": "^1.8.2",
    "object-assign": "4.1.1",
    "path": "^0.12.7",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "raf": "3.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.7",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "swagger-ui-express": "^1.0.8",
    "whatwg-fetch": "2.0.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chakram": "^1.5.0",
    "css-loader": "0.28.7",
    "dotenv": "^4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "eslint-watch": "^3.1.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "html-webpack-plugin": "2.29.0",
    "mocha": "^3.5.3",
    "moment": "^2.19.1",
    "node-sass": "^4.6.1",
    "nodemon": "^1.12.0",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "pre-commit": "^1.2.2",
    "react": "^16.0.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.0.0",
    "react-moment": "^0.6.5",
    "redux-immutable-state-invariant": "^2.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/client/build/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/client/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/client/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/client/build/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/client/build/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
